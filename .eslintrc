{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "linebreak-style": "off",
    "keyword-spacing": ["error", { "before": true, "after": true , "overrides": {
      "if": { "after": false }
    }}],
    "consistent-return": "off",
    "max-classes-per-file": ["error", 5],
  }
}
